{% extends 'base.html.twig' %}
{% block title 'Company' %}
{% block pageTitle 'Tree' %}
{% block pageSubtitle 'Tree of employee' %}

{% block body %}
    <div class="row">
        <p>
            {% for user in usersTree %}
                {{ user['tree'] }}&nbsp;<label><input id="{{ user['id'] }}" type="checkbox" checked></label>&nbsp;{{ user['name'] }} <br />
            {% else %}
                No user found
            {% endfor %}
        </p>
    </div>
{% endblock %}
